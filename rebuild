docker rm -f $(docker ps -q --filter name=srsRTMP)
docker build -t srs-rtmp:0.1 .
docker run --name srsRTMP -p 8085:1935 -p 8084:80 \
    -e ON_CONNECT_ENDPOINT=http://host.docker.internal:8086/Live/OnConnect \
    -e ON_PUBLISH_ENDPOINT=http://host.docker.internal:8086/Live/OnPublish \
    -e ON_UNPUBLISH_ENDPOINT=http://host.docker.internal:8086/Live/OnUnPublish \
    -e HTTP_URL=http://localhost:8084 \
    --restart unless-stopped srs-rtmp:0.1
docker exec -it $(docker ps -q --filter name=srsRTMP) /bin/bash
